<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.coinsfriendly.mobile"
          android:versionCode="5"
          android:versionName="1.0.3.2">
    <uses-sdk android:minSdkVersion="14"/>

    <uses-permission android:name="android.permission.INTERNET" />

    <application android:configChanges="orientation"
                 android:icon="@drawable/ic_launcher_cf"
                 android:label="@string/app_name"
                 android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen">

        <activity android:name=".graphs.GraphsMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".graphs.GraphsActivity"/>
        <activity android:name=".MenuActivity"/>
        <activity android:name=".PointOfSaleActivity"/>
        <activity android:name=".InviteActivity"/>
        <activity android:name=".TxHistoryActivity"/>
    </application>

</manifest>
